{
  "expo": {
    "name": "Koaffee",
    "slug": "Koaffee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/pass_logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/pass_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.PassExpo"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/pass_logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#B3D8DE"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
